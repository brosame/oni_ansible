short_hostname: ruri

wan_interface: eth1

olsr_main_ip: 192.168.10.12

olsr_interfaces:
  - { name: "eth0" }

olsr_nameservice_announcements:
  - "http://{{ olsr_main_ip }}:80|tcp|public-domain-http downloads.opennet-initiative.de"
  - "http://{{ olsr_main_ip }}:80|tcp|public-domain-http downloads.on-i.de"
  - "http://{{ olsr_main_ip }}:80|tcp|public-domain-http downloads.rostock.freifunk.net"
  - "http://{{ olsr_main_ip }}:80|tcp|public-domain-http downloads.schwerin.freifunk.net"
  - "http://{{ olsr_main_ip }}:80|tcp|public-domain-http downloads.wismar.freifunk.net"
  - "https://{{ olsr_main_ip }}:443|tcp|public-domain-https downloads.opennet-initiative.de"
  - "https://{{ olsr_main_ip }}:443|tcp|public-domain-https downloads.on-i.de"
  - "https://{{ olsr_main_ip }}:443|tcp|public-domain-https downloads.rostock.freifunk.net"
  - "https://{{ olsr_main_ip }}:443|tcp|public-domain-https downloads.schwerin.freifunk.net"
  - "https://{{ olsr_main_ip }}:443|tcp|public-domain-https downloads.wismar.freifunk.net"

letsencrypt_certificates:
- {primary: downloads.opennet-initiative.de, other: downloads.on-i.de downloads.rostock.freifunk.net downloads.schwerin.freifunk.net downloads.wismar.freifunk.net}
